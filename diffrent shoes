#include<bits\stdc++.h>
using namespace std ;
int main()
{   int count =0;
int i,j;
   int arr[4];
   for( i=0;i<4;i++)
   {
    cin>>arr[i];
   }
   sort(arr,arr+4);
    for( i=0;i<4;i++)
    {
            if(arr[i]!=arr[i+1])
            count++;       
    }
     cout<<4-count<<endl;
     
    
    
}
